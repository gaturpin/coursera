    "import numpy as np\n",
    "w = np.array([1.0, 2.5, -3.3])\n",
    "b = 4\n",
    "x = np.array([10,20,30])\n",
    "\n",
    "f = np.dot(w,x) + b #vectorisation - no for loop"
   ]
  },
